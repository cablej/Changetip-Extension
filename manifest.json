{
  "name": "Changetip Tip Button",
  "description": "Tip anyone online!",
  "version": "1.0",
  "permissions": ["tabs", "storage"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery.min.js", "changetip.js"],
      "css": ["changetip.css"]
    }
  ],
  "icons": {"128": "img/128.png" },
  "background": {
    "page": "background.html"
     },
  "options_page": "options.html",
  "web_accessible_resources": ["img/dollar_sign.png", "img/dollar_sign_hover.png", "jquery.min.js", "img/dollar_sign_soundcloud.png"],
  "manifest_version": 2,
  "browser_action": {
    "default_title": "Changetip Extension",
    "default_popup": "options.html"
  }
}
